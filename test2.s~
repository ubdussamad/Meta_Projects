#include <asm/unistd.h>
#include <syscall.h>	

	.file	"test2.cpp"
	.section	.rodata
hello:
	.string	"HELLO WORLD\n"
	.text
	.globl	main
	.type	main, @function
main:
	pushq	%rbp
	movq	%rsp, %rbp


	## Writing
	movl	$14, %edx
	movl	$hello, %esi
	movl	$1, %edi
	movl	$SYS_write, %eax
	syscall

	## exit the program and handle the control to parent
	movl	$0, %eax
	popq	%rbp
	ret
