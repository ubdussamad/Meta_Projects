#include <unistd.h>
#include <sys/syscall.h>
#incldue <iostream>
#define STDOUT__ 0x1
int main (  void )  {
  syscall ( SYS_write,  STDOUT__ , "HELLO WORLD\n"  , 12 );
  std::cout << "Hoi" << std::endl;
  flt_to_Sflt(5);
  return(0);
}

void flt_to_Sflt ( float num ) {
  num = num>>10;
  return( num );
}
